https://www.geeksforgeeks.org/batch/skill-up-dsa/track/su-dsa-daily-week1-day1/problem/divisible-by-13

class Solution {
    public boolean divby13(String s) {
        // code here
        int rem=0;
        for(int i=0;i<s.length();i++)
        {
            int n=s.charAt(i)-'0';
            rem=(rem*10+n)%13;
        }
        return rem==0;
    }
}
